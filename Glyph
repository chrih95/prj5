package prj5;

import CS2114.SquareShape;
import java.awt.Color;

/**
* Creates a new glyph.
* @author Christian Hawkes
*/
public SquareShape glyph;

    /**
    * Creates and returns a new square shape.
    * @return The new glyph
    */
    public SquareShape buildGlyph() {
        glyph = new SquareShape(150, 50, 150, Color.WHITE);
        return glyph;
    }
