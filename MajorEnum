package prj5;

/**
* An Enumeration with COMPSCI, MATH, ENGINEERING, and OTHER
* representing the major of each person.
*/
public enum MajorEnum {
    COMPSCI(0), MATH(1), ENGINEERING(2), OTHER(3);

    private final int arrayIndex;

    public int getArrayIndex() {

        return this.arrayIndex;
    }

    private MajorEnum(int arrayIndex) {
        this.arrayIndex = arrayIndex;
    }
}
